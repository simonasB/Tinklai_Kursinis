+    float:left;
+    width:300px;
+    height:300px;
+    overflow:hidden;
+    border:6px solid #fff;
+    background:#fff;
+
+    -webkit-border-radius:150px;
+       -moz-border-radius:150px;
+            border-radius:150px;
+    -webkit-box-shadow:0px 0px 5px #ccc;
+            box-shadow:0px 0px 5px #ccc;
